<div class="header mb-5">
    <nav id="nav-main" class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
             {% if function('has_custom_logo') %}
                <a href="{{ site.url }}" class="custom-logo-link" aria-label="{{ site.name }}">
                    {{ fn('the_custom_logo') }}
                </a>
            {% else %}
                <a href="{{ site.url }}">{{ site.name }}</a>
            {% endif %}
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    {% include 'partials/menu.twig' with {
                        items: main_menu.get_items
                    } %}
                </ul>
            </div>
        </div>
    </nav>
</div>
